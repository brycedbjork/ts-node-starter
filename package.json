{
  "name": "has-api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "source .env && babel-node ./src/app.ts --extensions '.ts'",
    "start": "yarn build && node ./build/app",
    "build": "yarn clean && yarn build-babel",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "./node_modules/.bin/babel -d ./build ./src -s --extensions '.ts'",
    "check-types": "tsc"
  },
  "dependencies": {
    "@sentry/node": "^5.4.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "debug": "~4.1.1",
    "express": "~4.17.1",
    "firebase-admin": "^8.2.0",
    "jimp": "^0.6.4",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "twilio": "^3.32.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/morgan": "^1.7.36",
    "typescript": "^3.5.3"
  }
}
