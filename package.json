{
  "name": "has-api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "source .env && ./node_modules/nodemon/bin/nodemon.js",
    "start": "ts-node src/app.ts",
    "build": "yarn clean && yarn build-babel",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "npx babel -d ./build ./src -s --extensions '.ts' --ignore src/schemas",
    "check-types": "tsc"
  },
  "dependencies": {
    "@sendgrid/mail": "^6.4.0",
    "@sentry/node": "^5.4.3",
    "atob": "^2.1.2",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "debug": "~4.1.1",
    "express": "~4.17.1",
    "firebase-admin": "^8.2.0",
    "geofirestore": "^3.3.1",
    "jimp": "^0.6.4",
    "lodash": "^4.17.21",
    "moment": "^2.24.0",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "stripe": "^7.6.0",
    "twilio": "^3.33.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/plugin-transform-typescript": "^7.6.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/atob": "^2.1.2",
    "@types/morgan": "^1.7.36",
    "@types/stripe": "^6.31.13",
    "nodemon": "^1.19.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  }
}
